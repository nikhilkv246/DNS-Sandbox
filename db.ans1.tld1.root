;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	nsa.ans1.tld1.root. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	nsa.ans1.tld1.root.
nsa	IN	A	172.20.2.8
@	IN	A	172.20.2.8

ex1	IN	A	1.2.3.4
ex2	IN	A	5.6.7.8
ex3	IN	CNAME	ex1

